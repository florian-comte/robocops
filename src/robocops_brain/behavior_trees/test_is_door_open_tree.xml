<root BTCPP_format="4" main_tree_to_execute="Main">
  <BehaviorTree ID="Main">
    <Sequence>
      <SearchAndGrab 
        zone="4" 
        timeout_duration="60" 
        current_grabbed_zones="{current_grabbed_zones}" 
        never_timed_out_zones="{never_timed_out_zones}" 
        current_inventory="{current_inventory}" 
        second_pose_x="3.50"
        second_pose_y="5.0"
        second_pose_yaw="0"
      />
    </Sequence>
  </BehaviorTree>
</root>