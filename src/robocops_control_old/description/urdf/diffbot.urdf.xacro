<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robocops_2_wheels">
    <xacro:arg name="prefix" default="" />
    <xacro:include filename="robot_core.xacro" />

    <xacro:include filename="$(find robocops_control)/ros2_control/diffbot.ros2_control.xacro" />
    
    <xacro:diffbot_ros2_control name="DiffBot" prefix="$(arg prefix)"/>
